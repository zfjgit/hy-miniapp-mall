.view-aboutus {
  margin-bottom: 80rpx;
  width: 100%;
}
.view-aboutus-logos {
  width: 100%;
  height: 200rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #fff;
}
.view-aboutus-logo-img {
  width: 100%;
  height: 200rpx;
}
.view-aboutus-text {
  width: 95%;
  padding: 20rpx;
  text-align: left;
  text-indent: 2em;
  background-color: #fff;
  line-height: 50rpx;
  margin: 10rpx 0;
}

.view-main-copyright {
  width: 100%;
  height: 80rpx;
  padding: 20rpx 0;
  text-align: center;
  position: fixed;
  bottom: 0;
  background-color: #fff;
}
.view-copyright-poweredby {
  width: 100%;
  height: 35rpx;
  line-height: 35rpx;
  padding: 5rpx 0;
  color: #aaa;
}
.view-copyright-holder {
  width: 100%;
  height: 35rpx;
  line-height: 35rpx;
  padding: 5rpx 0;
}

